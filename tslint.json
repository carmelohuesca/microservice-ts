{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [
            false,
            "tabs"
        ],
        "max-classes-per-file": false,
        "quotemark": [
            true,
            "single"
